{
  "babelrcRoots": ["."],
  "include": ["./src/js/**/*"],
  "exclude": ["./src/js/**/*.map", "./src/js/**/*.min.js"],
  "presets": [
    [
      "@babel/preset-env",
      {
        "bugfixes": true,
        "corejs": {
          "version": "3.18",
          "proposals": true
        },
        "debug": false,
        "exclude": [],
        "forceAllTransforms": true,
        "targets": {
          "esmodules": true
        },
        "useBuiltIns": "usage"
      }
    ]
  ]
}
